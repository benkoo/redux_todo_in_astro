---
// src/layouts/ReactLayout.astro 
import { Provider } from 'react-redux';
import { store } from '../store';

interface Props {
  children: any;
}

const { children } = Astro.props;

---
<html>   
	<body>     
		<main>       
			<Provider store={store} client:load>
				<slot />
			</Provider>     
		</main>     
		<aside>       
			<slot name="sidebar" />     
		</aside>   
	</body> 
</html>